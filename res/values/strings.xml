    <!-- several material have the same equation for thermal conductivity, specific heat, coefficient expansion  -->
    <string name="thermal_conductivity_or_specific_heat_or_coefficient_expansion_equation_common">
        y = 10 <small><small><sup>(a + b(log<small><sub>10</sub></small>T) + 
         c(log<small><sub>10</sub></small>T) <small><sup>2</sup></small> +
         d(log<small><sub>10</sub></small>T) <small><sup>3</sup></small> + 
         e(log<small><sub>10</sub></small>T) <small><sup>4</sup></small> + 
         f(log<small><sub>10</sub></small>T) <small><sup>5</sup></small> + 
         g(log<small><sub>10</sub></small>T) <small><sup>6</sup></small> + 
         h(log<small><sub>10</sub></small>T) <small><sup>7</sup></small> + 
         i(log<small><sub>10</sub></small>T) <small><sup>8</sup></small>) 
         </sup> </small></small>
         </string>
    <string name="thermal_conductivity_equation_copper">
        y = 10 <small><small><sup>(a + cT<small><sup>0.5</sup></small> + 
         eT + gT<small><sup>1.5</sup></small> + iT<small><sup>2</sup></small>) / 
         (1 + bT<small><sup>0.5</sup></small> + 
         dT + fT<small><sup>1.5</sup></small> + hT<small><sup>2</sup></small>)
         </sup> </small></small>
         </string>
     
    <string name="thermal_conductivity_equation_kevlar_49">
        y = Log<small><sub>10</sub></small>( 
         ( a + b(log<small><sub>10</sub></small>T))
         ((1 - <i> erf* </i>(2(log<small><sub>10</sub></small>T - c)))/2) + 
         (d+e (exp(-x/f)))
         (1 + <i> erf* </i>(2(log<small><sub>10</sub></small>T - c)))/2)
         ) \n\n <small>*erf = Gaussian error function</small> \n
         </string>
    
     <string name="young_modulus_equation_common">
        y = a + bT + cT<small><sup>2</sup></small> +
         dT<small><sup>3</sup></small> + 
         eT<small><sup>4</sup></small> 
         </string>
         
     <string name="linear_expansion_equation_common">            
        T<sub><small><small>low</small></small></sub> > T \t 
         y = f> \n
        T > T<sub><small><small>low</small></small></sub> \t
         y = a + bT + cT<small><sup>2</sup></small> +
         dT<small><sup>3</sup></small> + 
         eT<small><sup>4</sup></small> 
         </string>
    
